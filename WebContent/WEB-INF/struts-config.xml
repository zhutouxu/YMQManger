<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="loginForm" type="com.hundsun.xuxp10575.struts.form.LoginActionForm"></form-bean>
		<form-bean name="AddUserForm" type="com.hundsun.xuxp10575.struts.form.AddVipUserForm"></form-bean>
		<form-bean name="AddYMQApplyForm" type="com.hundsun.xuxp10575.struts.form.AddYMQApplyForm"></form-bean>
		<form-bean name="QryActivityUserForm" type="com.hundsun.xuxp10575.struts.form.QryActivityUserForm"></form-bean>
		<form-bean name="YMQSignInForm" type="com.hundsun.xuxp10575.struts.form.YMQSignInForm"></form-bean>
		<form-bean name="ChangePwdForm" type="com.hundsun.xuxp10575.struts.form.ChangePwdForm"></form-bean>
		<form-bean name="QryVipUserForm" type="com.hundsun.xuxp10575.struts.form.QryVipUserForm"></form-bean>
		<form-bean name="GetApplyInfoForm" type="com.hundsun.xuxp10575.struts.form.GetApplyInfoForm"></form-bean>
	</form-beans>	
	<action-mappings>
		<!-- 以下是管理员用户使用的action -->
		<action path="/login" 
				type="com.hundsun.xuxp10575.struts.action.LoginAction"
				name="loginForm"
				scope="session"
				>
			<forward name="success" redirect="true" path="index.html"></forward>
			<forward name="error" redirect="true" path="/page/Hellologin.html"></forward>
		</action>	
		<action path="/changePwd" 
				type="com.hundsun.xuxp10575.struts.action.ChangePwdAction"
				name="ChangePwdForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/logout" 
				type="com.hundsun.xuxp10575.struts.action.LogoutAction"
				name=""
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>		
		<action path="/addvipuser" 
				type="com.hundsun.xuxp10575.struts.action.AddVipUserAction"
				name="AddUserForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/qryvipuser" 
				type="com.hundsun.xuxp10575.struts.action.QryVipUserAction"
				name="QryVipUserForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/updatevipuser" 
				type="com.hundsun.xuxp10575.struts.action.UpdateVipUserAction"
				name="AddUserForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/delvipuser" 
				type="com.hundsun.xuxp10575.struts.action.DelVipUserAction"
				name="QryVipUserForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/getapplyinfo" 
				type="com.hundsun.xuxp10575.struts.action.GetApplyInfoAction"
				name="GetApplyInfoForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>	
		<action path="/getSignUpFields" 
				type="com.hundsun.xuxp10575.struts.action.QryYMQFieldAction"
				name=""
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>								
	    <!-- 以下是周边普通用户使用的action -->							
		<action path="/cancelSignUp" 
				type="com.hundsun.xuxp10575.struts.action.CancelYMQApplyAction"
				name="YMQSignInForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>	
		<action path="/submitSignIn" 
				type="com.hundsun.xuxp10575.struts.action.YMQSignInAction"
				name="YMQSignInForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>		
		<action path="/getActivitieUsers" 
				type="com.hundsun.xuxp10575.struts.action.QryActivityUserAction"
				name="QryActivityUserForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>	
		<action path="/getSignUpTimes" 
				type="com.hundsun.xuxp10575.struts.action.QryActivityTimeAction"
				name=""
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>
		<action path="/searchSignUp" 
				type="com.hundsun.xuxp10575.struts.action.QryApplyInfoAction"
				name="YMQSignInForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>				
		<action path="/submitSignUp" 
				type="com.hundsun.xuxp10575.struts.action.AddYMQApply"
				name="AddYMQApplyForm"
				scope="request"
				>
			<forward name="success" redirect="false" path=""></forward>
			<forward name="error" redirect="false" path=""></forward>
		</action>						
	</action-mappings>
</struts-config>
